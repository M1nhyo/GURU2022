<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.techtown.feelim">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Feelim">
        <activity android:name=".listViewItem"></activity>
        <activity android:name=".listViewAdapter" />
        <activity android:name=".myfeelimlist" />
        <activity
            android:name=".MainActivity3"
            android:label="@string/title_activity_main3"
            android:theme="@style/Theme.Feelim.NoActionBar" />
        <activity android:name=".MainActivity2" />
        <activity android:name=".myfeelim02" />
        <activity android:name=".myFeelim" />
        <activity
            android:name=".newFeelim"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".infoFeelim"
            android:windowSoftInputMode="stateHidden|adjustPan" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".moreInfo" />
    </application>

</manifest>